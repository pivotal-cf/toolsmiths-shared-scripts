{
  ".properties.security_acknowledgement": {
    "type": "string",
    "value": "I agree"
  },
  ".mysql_monitor.recipient_email": {
    "type": "string",
    "value": "bogus@this.is.just.a.placeholder.pivotal.io"
  },
  ".cloud_controller.system_domain": {
    "type": "wildcard_domain",
    "value": "sys.<%= ENV.fetch('DOMAIN') %>"
  },
  ".cloud_controller.apps_domain": {
    "type": "wildcard_domain",
    "value": "apps.<%= ENV.fetch('DOMAIN') %>"
  },
  ".ha_proxy.static_ips": {
    "type": "ip_ranges",
    "value": "<%= ENV.fetch('HAPROXY_IP') %>"
  },
  ".ha_proxy.skip_cert_verify": {
    "type": "boolean",
    "value": true
  },
  <%- if ( Gem::Version.new(ENV.fetch('ERT_VERSION')) > Gem::Version.new('1.9.9999')) -%>
  ".properties.credhub_key_encryption_password": {
    "value": {
      "secret": <%= (ENV.fetch('DEFAULT_OPSMAN_PASSWORD').dump * 3)[0..25] %>
    }
  },
  <%- end -%>
  <%- if ( Gem::Version.new(ENV.fetch('ERT_VERSION')) < Gem::Version.new('1.12')) -%>
  ".properties.networking_point_of_entry.external_ssl.ssl_ciphers": {
    "value": "TLS_ECDHE_RSA_WITH_AES_128_GCM_SHA256:TLS_ECDHE_RSA_WITH_AES_256_GCM_SHA384"
  },
  <%- end -%>
  <%- if Gem::Version.new(ENV.fetch('ERT_VERSION')) > Gem::Version.new('1.11.9999') -%>
  ".properties.haproxy_forward_tls": {
    "value": "disable"
  },
  ".properties.gorouter_ssl_ciphers": {
    "value": "TLS_RSA_WITH_RC4_128_SHA:TLS_RSA_WITH_AES_128_CBC_SHA"
  },
  ".properties.haproxy_ssl_ciphers": {
    "value": "DHE-RSA-AES128-GCM-SHA256:DHE-RSA-AES256-GCM-SHA384:ECDHE-RSA-AES128-GCM-SHA256:ECDHE-RSA-AES256-GCM-SHA384"
  },
  ".properties.networking_poe_ssl_cert": {
  <%- else -%>
  ".properties.networking_point_of_entry.haproxy.ssl_rsa_certificate": {
  <%- end -%>
    "value": {
      "private_key_pem": <%= ENV.fetch('DOMAIN_KEY').dump %>,
      "cert_pem": <%= ENV.fetch('DOMAIN_CRT').dump %>
    }
  }
}
