{
  ".properties.security_acknowledgement": {
    "type": "string",
    "value": "I agree"
  },
  ".mysql_monitor.recipient_email": {
    "type": "string",
    "value": "bogus@this.is.just.a.placeholder.pivotal.io"
  },
  ".cloud_controller.system_domain": {
    "type": "wildcard_domain",
    "value": "sys.<%= ENV.fetch('DOMAIN') %>"
  },
  ".cloud_controller.apps_domain": {
    "type": "wildcard_domain",
    "value": "apps.<%= ENV.fetch('DOMAIN') %>"
  },
  ".ha_proxy.static_ips": {
    "type": "ip_ranges",
    "value": "<%= ENV.fetch('HAPROXY_IP') %>"
  },
  ".ha_proxy.skip_cert_verify": {
    "type": "boolean",
    "value": true
  },
  <%- if Gem::Version.new(ENV.fetch('ERT_VERSION').gsub('\\','').gsub('..*','')) >= Gem::Version.new('1.12') -%>
  ".properties.haproxy_forward_tls": {
    "value": "disable"
  },
  ".properties.networking_poe_ssl_certs": {
    "value": [{
      "name": "self-signed",
      "private_key": <%= ENV.fetch('DOMAIN_KEY').dump %>,
      "cert_chain": <%= (ENV.fetch('DOMAIN_CRT') + "\n").dump %>
    }]
  }
  <%- else -%>
    ".properties.networking_point_of_entry.haproxy.ssl_rsa_certificate": {
      "value": {
        "private_key_pem": <%= ENV.fetch('DOMAIN_KEY').dump %>,
        "cert_pem": <%= ENV.fetch('DOMAIN_CRT').dump %>
      }
    }
    <%- end -%>
}
